<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.god.dream.boss.dao.region.RegionDao">

    <resultMap id="region" type="com.god.dream.boss.dao.region.RegionDao">
        <id property="id" column="id"/>
        <result property="regionKey" column="region_key"/>
        <result property="regionName" column="region_name"/>
    </resultMap>

    <select id="find" parameterType="long" resultMap="region">
        select id, region_key, region_name from crm_region where id = #{0}
    </select>

</mapper>
